    "electron": "^15.3.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "less": "^4.1.2",
    "less-loader": "10.2.0",
    "node-loader": "^2.0.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^3.3.1",
